#
# tauOS
#
	
# disable sshd socket by default on home and workstation
disable sshd.socket
	
# disable sshd service by default on home and workstation
disable sshd.service
	
# enable cups on-demand socket activation by default on home and workstation
# https://pagure.io/fedora-workstation/issue/8
enable cups.socket
enable cups.path
disable cups.service
	
# enable low-memory-monitor for GMemoryMonitor API
enable low-memory-monitor.service